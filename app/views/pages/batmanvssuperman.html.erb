<div class="jumbotron"> 
  <h1>Heroes</h1> 
  <div>
    <%= image_tag 'batman', size:'600x400' %>

<%= form_tag pages_save_heroe_path, method: 'post', class: 'form' do %>

    <div class="form-group">
      <%= label_tag :heroe, 'Heroe' %>
      <%= text_field_tag :heroe, nil, class: 'form-control' %>
    </div>

    <div class="form-group">
      <%= label_tag :email, 'Email' %>
      <%= text_field_tag :email, nil, class: 'form-control' %>
    </div>

    <div class="form-group">
